column_mean <- function(x, removeNA=TRUE)
{
cols <- ncol(x)
means <- numeric(cols)
for(i in 1:cols)
{
means[i] <- mean(x[, i], na.rm = removeNA)
}
means
}
column_mean(airquality)
column_mean(airquality, FALSE)
